@model IEnumerable<Labaratorinis23.Models.Section>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.sectionName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sectionDec)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Module.moduleName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.sectionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sectionDec)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Module.moduleName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.sectionID }) |
            @Html.ActionLink("Details", "Details", new { id=item.sectionID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.sectionID })
        </td>
    </tr>
}

</table>
